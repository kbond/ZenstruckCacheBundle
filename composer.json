{
    "name": "zenstruck/cache-bundle",
    "description": "Provides a httpcache warmup command for Symfony2",
    "keywords": ["cache", "caching", "httpcache"],
    "homepage": "http://zenstruck.com/project/ZenstruckCacheBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.5|~3.0",
        "symfony/console": "~2.5|~3.0",
        "php-http/httplug": "~1.0@dev",
        "php-http/message-factory": "~0.3",
        "php-http/discovery": "~0.2"
    },
    "require-dev": {
        "symfony/dom-crawler": "~2.5|~3.0",
        "symfony/css-selector": "~2.5|~3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.4",
        "php-http/guzzle5-adapter": "~0.2@dev",
        "guzzlehttp/psr7": "~1.0",
        "sllh/php-cs-fixer-styleci-bridge": "~1.4"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\CacheBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\CacheBundle\\Tests\\": "tests/" }
    },
    "suggest": {
        "dpn/xml-sitemap-bundle": "Create a sitemap with Symfony",
        "guzzlehttp/psr7": "To use and auto discover GuzzleMessageFactory",
        "php-http/guzzle5-adapter": "To use and auto discover Guzzle5HttpAdapter",
        "php-http/guzzle6-adapter": "To use and auto discover Guzzle6HttpAdapter"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
